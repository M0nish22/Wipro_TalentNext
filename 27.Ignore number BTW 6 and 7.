import java.util.*;
class prog27{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int a=sc.nextInt();
int arr[]=new int[a];
for(int i=0;i<a;i++){
arr[i]=sc.nextInt();
}
int sum=0;
boolean b=false;
for(int num:arr){
if (num==6){
b=true;
}
if(b==false){
sum+=num;
}
if(b==true && num==7){
b=false;
}
}
System.out.print(sum);	
}
}
