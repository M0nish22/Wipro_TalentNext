import java.util.*;
class prog23{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<=n-1;i++){
a[i]=sc.nextInt();
}
int f=sc.nextInt();
int i=-1;
for(int j=0;j<=n-1;j++){
if(a[j]==f){
i=j;
break;
}
}
System.out.println(i);
}
}
