import java.util.Scanner;
class prog21{
public static void main(String[] args){
Scanner sc=new Scanner(System.in);
int b=0;
int n=sc.nextInt();
int a[]=new int[n];
for(int i=0;i<=n-1;i++){
a[i]=sc.nextInt();
}
for(int i=0;i<=n-1;i++){
b=b+a[i];
}
int c=b/n;
System.out.println("sum"+b);
System.out.println("avg"+c);
}
}
